        <script><![CDATA[function showMessage(type, message) {
    var span = document.createElement('span');
    span.setAttribute('data-type', type);
    span.setAttribute('data-text', message);
    var notification = {
        xml: span
    };
    GlideUI.get().fire(new GlideUINotification(notification));
}

function addTestToTestSet() {
    var tests = g_list.getChecked();
    if(tests == "") {
		var msg = getMessage('Please select a test');
        showMessage('error', msg);
    } else {
        var dialogClass = GlideModal ? GlideModal : GlideDialogWindow;
        var dlg = new dialogClass("x_90350_test_track_copy_to_project_dialog");
        var titleMsg = getMessage("Select Test Set");
        dlg.setTitle(titleMsg);
        dlg.setPreference('closeDlgFunc', function() {dlg.destroy();});
		dlg.setPreference('sysparm_test_ids', tests);
        dlg.setSize(400,300);
        dlg.render();
    }
}]]></script>
